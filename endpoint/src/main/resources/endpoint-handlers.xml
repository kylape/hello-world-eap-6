<handler-chains xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:ns1="http://org.jboss.ws/jaxws/handlerscope"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee javaee_web_services_1_2.xsd">
  <handler-chain>
    <protocol-bindings>##SOAP11_HTTP</protocol-bindings>
    <handler>
      <handler-name>C</handler-name>
      <handler-class>com.redhat.gss.handler.HandlerC</handler-class>
    </handler>
    <handler>
      <handler-name>D</handler-name>
      <handler-class>com.redhat.gss.handler.HandlerD</handler-class>
    </handler>
  </handler-chain>
</handler-chains>
